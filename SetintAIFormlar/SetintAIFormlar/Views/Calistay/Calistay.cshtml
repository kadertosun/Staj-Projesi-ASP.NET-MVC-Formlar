
@{
    ViewBag.Title = "Calistay";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">


    <!-- Font Icon -->
    <link rel="stylesheet" href="~/colorlib-regform-15/fonts/material-icon/css/material-design-iconic-font.min.css">

    <!-- Main css -->
    <link rel="stylesheet" href="~/colorlib-regform-15/css/style.css">
</head>
<body>

    <div class="main">
        <div class="container">
            <div class="signup-content">
                <div class="signup-img">
                    <img src="~/colorlib-regform-15/images/cantarell.jpg" alt="">
                </div>
                <div class="signup-form">
                    <form method="POST" class="register-form" id="register-form">
                        <h2>Workshop Registration Form</h2>
                        <div class="form-row">
                            <div class="form-group">
                                <label for="adi">Name :</label>
                                <input type="text" name="adi" id="adi" required />
                            </div>
                            <div class="form-group">
                                <label for="soyadi">Surname :</label>
                                <input type="text" name="soyadi" id="soyadi" required />
                            </div>


                        </div>
                        <div class="form-row">
                            <div class="form-group">
                                <label for="unvani">Degree:</label>
                                <select name="unvani" id="unvani">
                                    <option value=""></option>
                                    <option>Prof.</option>
                                    <option>Dr.</option>
                                    <option>PhD.</option>
                                    <option>Atty.</option>
                                    <option>Eng.</option>

                                </select>
                            </div>
                            <div class="form-group">
                                <label for="kurumu">Corporation :</label>
                                <input type="text" name="kurumu" id="kurumu" required />
                            </div>

                        </div>
                        <div class="form-row">
                            <div class="form-group">
                                <label for="gorevi">Job:</label>
                                <input type="text" name="gorevi" id="gorevi" required />
                            </div>
                            <div class="form-group">
                                <label for="telefon">Phone:</label>
                                <input type="number" name="telefon" id="telefon" required />
                            </div>
                            <div class="form-group">
                                <label for="mail">E-mail:</label>
                                <input type="email" name="mail" id="mail" required />
                            </div>

                        </div>

                        <div class="form-group">
                            <label for="katilim">Reason to Join :</label>
                            <textarea type="text" name="katilim" id="katilim" required cols="80" rows="10"> </textarea>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="form-check">
                                    <input type="checkbox" name="kabul" id="agree-term" class="agree-term" required />
                                    <label for="kabul" class="label-agree-term">
                                        <span><span></span></span>I accept the SETINT AI KVKK Policy.
                                    </label>
                                    <input type="checkbox" name="agree-term" id="kabul" class="agree-term" required />
                                    <label for="kabul" class="label-agree-term">
                                        <span><span></span></span>I accept SETINT AI Medical disclaimer.
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-submit">
                            <input type="reset" value="Reset" class="submit" name="reset" id="reset" />
                            <input type="submit" value="Submit" class="submit" name="submit" id="submit" />
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>
    <!-- JS -->
    <script src="~/colorlib-regform-15/vendor/jquery/jquery.min.js"></script>
    <script src="~/colorlib-regform-15/js/main.js"></script>
</body><!-- This templates was made by Colorlib (https://colorlib.com) -->
</html>

